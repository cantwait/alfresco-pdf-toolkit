<alfresco-config>
	<config evaluator="string-compare" condition="Action Wizards">
		<action-handlers>
				<handler name="pdf-split" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFSplitActionHandler" />
				<handler name="pdf-append" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFAppendActionHandler" />
				<handler name="pdf-insert-at-page" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFInsertAtPageActionHandler" />
				<handler name="pdf-split-at-page" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFSplitAtPageActionHandler" />
				<handler name="pdf-watermark" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFWatermarkActionHandler" />
				<handler name="pdf-encryption" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFEncryptionActionHandler" />
				<handler name="pdf-signature" class="org.alfresco.extension.pdftoolkit.web.bean.actions.handlers.PDFSignatureActionHandler" />
		</action-handlers>
	</config>
</alfresco-config>
